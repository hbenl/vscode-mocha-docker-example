{
  "files.exclude": {
    "node_modules": true
  },
  "editor.insertSpaces": false,
  "editor.tabSize": 4,
  "[json]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "mochaExplorer.launcherScript": "node_modules/mocha-explorer-launcher-scripts/docker",
  "mochaExplorer.env": {
    "DOCKER_IMAGE": "node:current-alpine",
    "DOCKER_EXTRA_ARGS": "[\"-h\",\"mocha-container\"]",
    "TEST_ENVVAR": "TEST_VALUE"
  },
  "mochaExplorer.require": [ "required.js" ],
  "mochaExplorer.debuggerConfig": "Debug in Docker",
}